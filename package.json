  "files": [
    "build"
  ],
  "bin": {
    "dash-button": "build/cli.js"
  },
  "enginesStrict": {
    "node": ">=8.3"
  },
  "scripts": {
    "build": "gulp build",
    "flow": "flow",
    "lint": "eslint src",
