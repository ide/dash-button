{
  "name": "dash-button",
  "version": "1.0.0",
  "description": "A small server that reacts to Amazon Dash buttons on your WiFi network",
  "main": "build/DashButton.js",
  "files": [
    "build"
  ],
  "bin": {
    "dash-button": "build/cli.js"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "gulp build",
    "lint": "eslint src",
    "prepublish": "gulp clean && gulp build",
    "test": "jest",
    "watch": "gulp watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ],
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "unmockedModulePathPatterns": [
      "/node_modules/babel-runtime/",
      "/node_modules/lodash/",
      "/node_modules/pcap/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ide/dash-button.git"
  },
  "keywords": [
    "amazon",
    "dash",
    "button"
  ],
  "author": "James Ide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ide/dash-button/issues"
  },
  "homepage": "https://github.com/ide/dash-button#readme",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "lodash": "^4.15.0",
    "pcap": "mranney/node_pcap#4634a39",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.4.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-flow-strip-types": "^6.4.0",
    "babel-plugin-transform-function-bind": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.12.0",
    "eslint": "^3.3.1",
    "eslint-config-exponent": "^3.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.1",
    "gulp-changed": "^1.3.2",
    "jest-cli": "^14.1.0",
    "rimraf": "^2.5.4"
  }
}
